{
  "name": "nthn.crlsn",
  "version": "1.0.0",
  "description": "Nathan's Personal Website",
  "main": "src/services/server/server.js",
  "scripts": {
    "build": "better-npm-run build",
    "coverage": "coveralls < coverage/lcov.info",
    "deploy": "npm run build -- -p",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "lint": "eslint *.js src",
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "test": "jest -c jest.json",
    "watch": "npm run build -- --watch",
    "watch-client": "better-npm-run watch-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamambidextrous/liiist.git"
  },
  "keywords": [
    "Node",
    "React",
    "Redux",
    "isomorphic"
  ],
  "author": "https://github.com/teamambidextrous",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nscarlson/crlsn/issues"
  },
  "homepage": "https://liiist.io",
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "amazon-product-api": "^0.4.3",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.15",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "clean-webpack-plugin": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "coveralls": "^2.12.0",
    "css-loader": "^0.27.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "express-xml-bodyparser": "^0.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "history": "^4.6.0",
    "jest-cli": "^19.0.2",
    "keymirror": "^0.1.1",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "marked": "^0.3.6",
    "mongoose": "^4.9.0",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "piping": "^1.0.0-rc.4",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.13.0",
    "react-helmet": "^5.0.2",
    "react-modal": "^1.7.3",
    "react-portal": "^3.0.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-devtools-extension": "^2.13.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "source-map-support": "^0.4.11",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.5.0",
    "webpack-isomorphic-tools": "^2.6.6"
  }
}
